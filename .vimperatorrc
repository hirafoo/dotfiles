noremap <c-k> gt
noremap <c-j> gT
noremap <c-c> <esc>
noremap <c-a> <c-v><c-a>
noremap j 5j
noremap k 5k
noremap l 5l
noremap h 5h
noremap <silent> <c-l> <c-l>:noh<cR>
inoremap <c-a> <c-v><c-a>
cnoremap <c-p> <up>
cnoremap <c-n> <down>

set focuscontent
set complete=lbS
set defsearch=g
set guioptions=brm
set hlsearch
set suggestengines=g
set titlestring=Firefox
set wildoptions=auto

qmark b http://b.hatena.ne.jp/hirafoo/
qmark g http://github.com/hirafoo
qmark h http://mail.live.com/
qmark m https://mail.google.com/
qmark r http://www.google.co.jp/reader/view/
qmark s http://www.google.co.jp/bookmarks/
qmark x http://mixi.jp/

:style -name commandline-ime chrome://* #liberator-commandline-command input {ime-mode: inactive;}

"autocmd LocationChange .* js modes.passAllKeys = false
"autocmd LocationChange www\\.google\\.(co\\.jp|com)/reader js modes.passAllKeys = true

hi StatusLine color:black; background:#DDD; font-weight:normal;
hi CompDesc color:#272; width:50%;
hi Preview color:#44C;

"""plugins
""http://svn.coderepos.org/share/lang/javascript/vimperator-plugins/trunk/
"sbmcommentsviewer.js
map b :viewSBMComments<CR>
let g:def_sbm_format = "id,timestamp,tagsAndComment"
let g:def_sbms = "h"
let g:sbm_comments_viewer_filter_nocomments="true"
hi Message  padding-left: 0;

"char-hints-mod2.js -r33247
let g:hintsio="io"
let g:hintchars="qwerasdfzxcv"

"forcefocuscontent.js

"feedSomeKeys_2.js
autocmd LocationChange .* :fmapc
autocmd LocationChange www\\.google\\.co\\.jp/reader :fmap! -vkey J j K k O o n v X p ?
autocmd LocationChange mail\.google\.com/mail :fmap c / y j k n p o u e x s r a # [ ] z ? gi gs gt gd ga gc

"appendAnchor.js
let g:auto_append_anchor = "true"

echo "loaded!"

