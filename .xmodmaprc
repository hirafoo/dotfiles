! keycode 102 Muhenkan
! keycode 36  Return
! keycode 100 Henkan_Mode
! keycode 37  Control_L
! keycode 66  Eisu_toggle
! keycode 9   Escape

! muhenkan to return
keycode 102 = Return

! henkan to ctrl
keycode 100 = Control_L
add control = Control_L

! capslock to esc
remove lock = Eisu_toggle
keycode 66 = Escape
